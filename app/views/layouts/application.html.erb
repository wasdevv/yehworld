    <%# Font Awesome Icons %>
    <script src="https://kit.fontawesome.com/63fb97220f.js" crossorigin="anonymous"></script>

    <%# My Favicon  %>
    <%= favicon_link_tag 'https://i.imgur.com/xnrz0pT.png' %>

  </head>

  <body>
    <%= render 'shared/navbar' %>
    <br>
    <br>
    <br>
    <%= yield %>
